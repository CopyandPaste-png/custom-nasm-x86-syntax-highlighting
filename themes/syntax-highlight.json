{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"activityBarBadge.background": "#007acc",
		"editor.background": "#1e1e1e",
		"editor.foreground": "#d4d4d4",
		"editor.inactiveSelectionBackground": "#3a3d41",
		"editor.selectionHighlightBackground": "#add6ff26",
		"editorIndentGuide.activeBackground": "#707070",
		"editorIndentGuide.background": "#404040",
		"input.placeholderForeground": "#a6a6a6",
		"list.activeSelectionIconForeground": "#ffffff",
		"list.dropBackground": "#383b3d",
		"menu.background": "#252526",
		"menu.foreground": "#cccccc",
		"ports.iconRunningProcessForeground": "#369432",
		"sideBarSectionHeader.background": "#00000000",
		"sideBarSectionHeader.border": "#cccccc33",
		"sideBarTitle.foreground": "#bbbbbb",
		"statusBarItem.remoteBackground": "#16825d",
		"statusBarItem.remoteForeground": "#ffffff",
		"tab.lastPinnedBorder": "#cccccc33",
	},
	"tokenColors": [
        {
            "name":"string",
                "scope":[
                    "nasm.string"
                ],
            "settings":{
                "foreground":"#CE9178"
            }
            },
            {
            "name":"general",
                "scope":[
                    "nasm.keywords"
                ],
            "settings":{
                "foreground":"#24bde3"
            }
            },
            {"name":"operators",
                "scope":[
                    "nasm.keywords.operators"
                ],
            "settings":{
                "foreground":"#00b37c"
            }},
            {
                "name":"builtin",
                "scope":[
                    "nasm.keywords.builtin.general"
                ],
                "settings":{
                    "foreground":"#54f542"}
            },
            {
                "name":"sections",
                "scope":[
                    "nasm.keywords.builtin.sections"
                ],
                "settings":{
                    "foreground":"#fc56e1"}
            },
			{
				"name": "entrypoint",
				"scope":[
					"nasm.keywords.builtin.entrypoint"
				],
				"settings": {
					"foreground": "#5765fa",
				}
			},
        
            {"name":"general",
                "scope":[
                    "nasm.function"
                ],
                "settings":{
                    "foreground":"#ffa238"
                }},
            {
                "name":"general",
                "scope":[
                    "nasm.register"
                ],
                "settings":{
                    "foreground":"#42b3f5"
                }
            },
            {
                "name":"variable",
                "scope":[
                    "nasm"
                ],
                "settings":{
                    "foreground":"#af73ff"
                }
            },
			{
				"name": "main function",
				"scope":[
					"nasm.keywords.main"
				],
				"settings": {
					"foreground": "#fa8373"
				}
			},
            {
                "name":"line",
                "scope":[
                    "comment"
                ],
                "settings":{
                    "foreground":"#2ebf55"
                }
            },{
                "name":"source",
                "scope":[
                    "string.quoted.double.single-line",
                    "string.quoted.double"
                ],
                "settings":{
                    "foreground":"#CE9178"
                }
                
                
            }
	]
}